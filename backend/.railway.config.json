{
  "build": {
    "skipCache": true,
    "builder": "dockerfile",
    "config": {
      "dockerfile": "Dockerfile.railway"
    }
  },
  "deploy": {
    "healthCheck": {
      "path": "/health",
      "interval": 10,
      "timeout": 5,
      "retries": 3
    },
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
